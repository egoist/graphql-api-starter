{
  "private": true,
  "name": "graphql-api-starter",
  "version": "1.0.0",
  "main": "dist/main.js",
  "author": "EGOIST <0x142857@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node dist/main",
    "dev": "yarn build --watch",
    "build": "rollup -c rollup.server.js",
    "migrate:save": "node scripts/with-env prisma migrate save --experimental",
    "migrate:up": "node scripts/with-env prisma migrate up --experimental",
    "g:prisma": "prisma generate"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,ts,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@hapi/iron": "^6.0.0",
    "@prisma/client": "2.6.2",
    "apollo-server-express": "2.17.0",
    "body-parser": "1.19.0",
    "class-validator": "0.12.2",
    "cookie": "^0.4.1",
    "dotenv": "8.2.0",
    "dotenv-expand": "^5.1.0",
    "express": "4.17.3",
    "graphql": "15.3.0",
    "jsonwebtoken": "8.5.1",
    "nanoid": "^3.1.12",
    "passport": "^0.4.1",
    "passport-github": "^1.1.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "8.3.3",
    "type-graphql": "1.0.0"
  },
  "devDependencies": {
    "@egoist/prettier-config": "^0.1.0",
    "@prisma/cli": "2.6.2",
    "@types/cookie": "^0.4.0",
    "@types/jsonwebtoken": "8.5.0",
    "@types/passport": "^1.0.4",
    "@types/passport-github": "^1.1.5",
    "@types/passport-google-oauth20": "^2.0.4",
    "cross-spawn": "^7.0.3",
    "husky": "^4.3.0",
    "lint-staged": "^10.3.0",
    "prettier": "^2.1.1",
    "rollup": "^2.26.11",
    "rollup-plugin-spawn": "^1.0.1",
    "rollup-plugin-typescript2": "^0.27.2",
    "typescript": "4.0.2"
  }
}
